function StopCastSoundTherold(p: player):
    set {_ability} to metadata value "lastspell" of {_p}
    if {_ability} = "Mind Blast":
        stop sound "skill.therold.mind_blast_precast" for all players in radius 2 around {_p}
    else if {_ability} = "Void Eruption":
        stop sound "skill.therold.void_eruption_precast" for all players in radius 2 around {_p}
    else if {_ability} = "Mind Flay":
        stop sound "skill.therold.mindflay_channel" for all players
        stop sound "skill.therold.mindflay_channel_whisper" for all players in radius 2 around {_p}
    else if {_ability} = "Vampiric Touch":
        stop sound "skill.therold.mindflay_channel_whisper" for all players in radius 2 around {_p}
    else if {_ability} = "Shadow Word: Void":
        stop sound "skill.therold.shadow_word_void_precast" for all players in radius 2 around {_p}

function TheroldDamage(p: player, basevalue: number, damageStat: number, damageScaling: number, versStat: number, versScaling: number) :: number:
    set {_damage} to {_basevalue} + ({damage::%{_p}%}/{_damageScaling}) + ({versatility::%{_p}%}/{_versScaling})
    if metadata value "Voidform" of {_p} is set:
        set {_damage} to {_damage}*1.2
    return {_damage}

function CheckIfDotted(p: player, target: entity) :: boolean:
    if metadata value "Shadow Word: Pain-%{_p}%" of {_target} is set:
        set {_bool} to true
    if metadata value "Vampiric Touch-%{_p}%" of {_target} is set:
        set {_bool} to true
    if {_bool} is not set:
        set {_bool} to false
    return {_bool}

function TwistOfFate(p: player):
    if metadata value "TwistOfFate" of {_p} is true:
        set metadata value "TwistOfFate" of {_p} to false
        while metadata value "TwistOfFateDuration" of {_p} > 0 seconds:
            set {_duration} to metadata value "TwistOfFateDuration" of {_p}
            remove 1 second from {_duration}
            set metadata value "TwistOfFateDuration" of {_p} to {_duration}
            wait 1 second
            if 0 seconds >= metadata value "TwistOfFateDuration" of {_p}:
                clear metadata value "TwistOfFateDuration" of {_p}
                clear metadata value "TwistOfFate" of {_p}
                stop loop

function ShadowWordVoidRecharge(p: player, cd: timespan):
    wait {_cd}
    set {_charges} to metadata value "ShadowWordVoidCharges" of {_p}
    add 1 to {_charges}
    set metadata value "ShadowWordVoidCharges" of {_p} to {_charges}
    loop all items in the inventory of {_p}:
        if 1 of loop-item = light gray dye named "<##613e82>Shadow Word: Void" with nbt from "{CustomModelData:4,ShadowWordVoid:1b}":
            if {_charges} > 0:
                set item amount of loop-item to {_charges}
                if {_charges} = 1:
                    set line 2 of lore of loop-item to "&f%{_charges}% Charge"
                else:
                    set line 2 of lore of loop-item to "&f%{_charges}% Charges"
                reset item cooldown of loop-item for {_p}
                clear cooldown "Shadow Word: Void%{_p}%"
            else:
                set line 2 of lore of loop-item to "&f%{_charges}% Charges"
            stop loop

