                                        set tag "CanInteract" of nbt of (slot {_slotIndex} of metadata value "gearEquip" of player) to true		
                                else:
                                    add "%nl%&f&oClick here to equip this item." to lore of slot {_slotIndex} of metadata value "gearEquip" of player
                                    set tag "CanInteract" of nbt of (slot {_slotIndex} of metadata value "gearEquip" of player) to true								
                            else:
