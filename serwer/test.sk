function Agnes():
	set {_x} to "9f4a2d79-828c-21be-a3c6-df116f91f2e0" parsed as entity
	equip citizen 6512 with a diamond pickaxe
	execute console command "npc select 6512"
	while {_x} is alive:
		loop all blocks in radius 4 around {_x}:
			if y-coordinate of loop-block > y-coordinate of {_x}:
				if loop-block is stone:
					if y-coordinate of loop-block = (y-coordinate of {_x} + 0.5):
						set {_xc} to x-coordinate of loop-block
						set {_yc} to y-coordinate of loop-block
						set {_zc} to z-coordinate of loop-block
						#if {_x}'s horizontal facing is north:
						#	remove 1 from {_zc}
						#if {_x}'s horizontal facing is east:
						#	add 1 to {_xc}
						#if {_x}'s horizontal facing is west:
						#	remove 1 from {_xc}
						#if {_x}'s horizontal facing is south:
						#	add 1 to {_zc}
						#execute console command "npc pathto %{_xc}% %{_yc}-1% %{_zc}%"
						move citizen 6512 to location of loop-block at speed 1
					make citizen 6512 look at loop-block
					set {_block} to true
					wait 2 ticks
					make {_x} break loop-block
					stop loop
				if loop-block is dirt or gravel:
					make citizen 6512 look at loop-block
					set {_block} to true
					make {_x} break loop-block
					stop loop
		#set {_xc} to (rounded down x-coordinate of block in front of {_x})
		#set {_yc} to (rounded down y-coordinate of block in front of {_x})
		#set {_zc} to (rounded down z-coordinate of block in front of {_x})
		#execute console command "npc pathto %{_xc}% %{_yc}% %{_zc}%"
		if block under block in front of {_x} is not air:
			if block under block in front of {_x} is not solid:
				make {_x} swing their main hand
				set block at block under block in front of {_x} to cobblestone
				remove 1 cobblestone from {_x}
		PlaceTorch({_x})
		if {_x} is alive:
			if {engagedc::%{_x}%} is set:
				if cooldown "spectrallightning%{_x}%" is finished:
					create cooldown "spectrallightning%{_x}%" for 20 seconds
					AgnesLightning({_x})
		stop loop if {agnesstop} is true
		if {_block} is set:
			wait 1 second
		else:
			wait 3 seconds
function PlaceTorch(e: entity):
	chance of 2%:
		if block at {_e} is air:
			if block west of block at {_e} is not torch:
				if block east of block at {_e} is not torch:
					if block north of block at {_e} is not torch:
						if block south of block at {_e} is not torch:
							if block under {_e} is stone:
								set block at {_e} to torch
								make {_e} swing their hand
function AgnesLightning(e: entity):
	loop 10 times:
		if {_e} is alive:
			execute console command "npc sneak"
			AgnesCynder({_e})
			wait 0.5 seconds
			execute console command "npc sneak"
			wait 1 second
function AgnesCynder(e: entity):
	play sound "entity.lightning_bolt.thunder" with volume 5 and pitch 2 at {_e}
	loop all entities in radius 10 around {_e}:
		if {mob::%loop-entity%} is true:
			add loop-entity to {_targs::*}
	set {_dmg} to (1 + {obrazenia::%{_e}%}/400 + {uniwersalnosc::%{_e}%}/1200)
	loop {_targs::*}:
		set {lastdmgcause::%loop-value%} to "<##7430ab>Widmowe Błyskawice"
		set {lastdmgcauseEN::%loop-value%} to "<##7430ab>Spectral Lightning"
		set loop-value's last damage cause to sweep attack
		set {lastdmgcauselore::%loop-value%::*} to "&8Umiejętność" and "&fInstant cast" and "&7Cooldown: &f20 sekund" and "&7Użycie: &fPPM" and "<##7430ab>Razi pobliskich wrogów w zasięgu &f10 <##7430ab>kratek serią" and "<##7430ab>błyskawic, zadając &f1 <##7430ab>obrażeń na błyskawicę." and "<##7430ab>Cynder może powtórzyć tę umiejętność maksymalnie" and "&f10 <##7430ab>razy zanim wejdzie na cooldown."
		set {lastdmgcauseloreEN::%loop-value%::*} to "&8Ability" and "&fInstant cast" and "&7Cooldown: &f20 seconds" and "&7Use: &fRNB" and "<##7430ab>Shocks enemies in radius of &f10 <##7430ab>blocks" and "<##7430ab>with bolts of lightning, dealing &f1 <##7430ab>damage per lightning." and "<##7430ab>Cynder can repeat this ability up to" and "&f10 <##7430ab>times before it goes on cooldown."
		make {_e} damage loop-value by 2*({_dmg})
	loop random number between 3 and 5 times:
		set {_counter} to 1
		loop random number between 1 and 4 times:
			set {_loc%{_counter}%} to location of block at {_e}'s head
			increase {_counter} by 1
			loop 10 times:
				loop 10 times:
					play 2 black dust with speed 0 at {_loc1}
					play 1 purple dust with speed 0 at {_loc1}
					increase x-coordinate of {_loc1} by random number between random number between 0 and -0.32 and random number between 0 and 0.40
					increase y-coordinate of {_loc1} by random number between random number between 0 and -0.19 and random number between 0 and 0.15
					increase z-coordinate of {_loc1} by random number between random number between 0 and -0.32 and random number between 0 and 0.40
					
