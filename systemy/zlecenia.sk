#opcjonalne kolory "<##b9ff69>Łatwe&7/<##f5e05b>Średnie&7/<##ff3d3d>Trudne"

#"&7Ostatnimi czasy mamy problem z dostawą kamienia," and "&7przydałaby się pomocna dłoń, jeśli tylko" and "&7jesteś w stanie pomóc." and " " and "&7Cel:" and "&f- 0/2000 Przynieś kamień" with nbt "{""minecraft:custom_model_data"":{floats:[2.0f]}}"


on rightclick on a player:
	if clicked player's name = "Magnus":
		set {@zlecenia} to chest inventory with 6 rows named "&0Zlecenia"
		set slot integers between 0 and 8 of ({@zlecenia}) to black stained glass pane named " "
		set slot integers between 45 and 53 of ({@zlecenia}) to black stained glass pane named " "
		set slot 9 and 17 and 18 and 26 and 27 and 35 and 36 and 44 of {@zlecenia} to black stained glass pane named " "
		if {zlecenia::%player%::*} is set:
			set {_slot} to 19
			loop 10 times:
				if {_slot} = 24:
					set {_slot} to 28 + (loop-number-5)
				else:
					add 1 to {_slot}
				set slot {_slot} of ({@zlecenia}) to {zlecenia::%player%::%loop-number%}
		else:
			set {_trudnosc::*} to "&aŁatwe" and "&eŚrednie" and "<##ff3d3d>Trudne"
			set {_zleceniodawcy::*} to {zleceniodawcy::*}
			set {_slot} to 19
			loop 10 times:
				if {_slot} = 24:
					set {_slot} to 28 + (loop-number-5)
				else:
					add 1 to {_slot}
				set {_zleceniodawca} to a random element of {_zleceniodawcy::*}
				remove {_zleceniodawca} from {_zleceniodawcy::*}
				if {_zleceniodawca} = "Torbjörn":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[2.0f]}}"
				if {_zleceniodawca} = "Akasaka":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[3.0f]}}"
				if {_zleceniodawca} = "Rina":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[8.0f]}}"
				if {_zleceniodawca} = "Sedriah":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[9.0f]}}"
				if {_zleceniodawca} = "Cassie":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[11.0f]}}"
				if {_zleceniodawca} = "Marcus":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[13.0f]}}"
				if {_zleceniodawca} = "Krysta":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[28.0f]}}"
				if {_zleceniodawca} = "Cornelia":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[31.0f]}}"
				if {_zleceniodawca} = "Zach":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[35.0f]}}"
					set {_profesja} to "JUBILERSTWO"
				if {_zleceniodawca} = "Czarek":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[36.0f]}}"
				if {_zleceniodawca} = "Magni":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[37.0f]}}" 
				if {_zleceniodawca} = "'Brodaty' Zachary":
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[38.0f]}}"
					set {_profesja} to "GÓRNICTWO"
				if {_nbt} is not set:
					set {_nbt} to "{""minecraft:custom_model_data"":{floats:[31.0f]}}"
				set {_zlecenie} to a random element of {zlecenie::%{_zleceniodawca}%::*}
				if {_zlecenie} = {zlecenie::%{_zleceniodawca}%::4}:
					if {_zleceniodawca} is not "Cassie" or "Artur":
						if {profesje::%player%::*} contains {_profesja}:
							set {_profspec::%loop-number%} to true
						else:
							while {_zlecenie} = {zlecenie::%{_zleceniodawca}%::4}:
								set {_zlecenie} to a random element of {zlecenie::%{_zleceniodawca}%::*}
								stop loop if {_zlecenie} is not {zlecenie::%{_zleceniodawca}%::4}
				set {_x} to a random element of {_trudnosc::*}
				if {_x} contains "Łatwe" or "Easy":
					set {_multiplier} to 1
				if {_x} contains "Średnie" or "Medium":
					set {_multiplier} to 2
				if {_x} contains "Trudne" or "Hard":
					set {_multiplier} to 3
				set slot {_slot} of ({@zlecenia}) to light gray stained glass pane named "&f%{_zleceniodawca}%" with lore "%{_x}%" with nbt {_nbt}
				add {_zlecenie} to lore of slot {_slot} of ({@zlecenia})
				if {_x} does not contain "Łatwe" or "Easy":
					loop lore of slot {_slot} of ({@zlecenia}):
						add 1 to {_loopnumber::%loop-number%}
						if loop-value-2 contains "&f- 0/":
							stop loop
					set {_lore::*} to line {_loopnumber::%loop-number%} of lore of slot {_slot} of ({@zlecenia}) split at " "
					set {_cel::*} to {_lore::2} split at "/"
					set {_cel} to {_cel::2} parsed as number
					if {_x} contains "Średnie" or "Medium":
						set {_cel} to {_cel}*2
					if {_x} contains "Trudne" or "Hard":
						set {_cel} to {_cel}*3
					set {_1} to {_lore::1}
					clear {_lore::1}
					clear {_lore::2}
					set {_lore::*} to join {_lore::*} by " "
					set line {_loopnumber::%loop-number%} of lore of slot {_slot} of ({@zlecenia}) to "%{_1}% %{_cel::1}%/%{_cel}% %{_lore::*}%"
				clear {_loopnumber::%loop-number%}
				if {language::%player%} = "English":
					set {_reputacja} to " &b%{_multiplier}*100% Reputation"
				else:
					set {_reputacja} to " &b%{_multiplier}*100% Reputacji"
				set {_nagroda} to a random integer between 1 and 6
				#cosmic flux, reputacja, stardust, 
				if {_nagroda} = 1:
					set {_color} to last color of {klejnot}'s name
					if {language::%player%} = "English":
						set {_nagroda} to " %{_color}%%{_multiplier}*25% %{klejnot}'s name%"
					else:
						set {_nagroda} to " %{_color}%%{_multiplier}*25% %{klejnot}'s name%ów"
				if {_nagroda} = 2:
					set {_color} to "&9"
					if {language::%player%} = "English":
						set {_nagroda} to " %{_color}%%{_multiplier}*1% %{boostersecretwonders}'s name%s"
					else:
						if {_multiplier} > 1:
							set {_nagroda} to " %{_color}%%{_multiplier}*1% &9Boostery &5Secret Wonders"
						else:
							set {_nagroda} to " %{_color}%%{_multiplier}*1% &9Booster &5Secret Wonders"
				if {_nagroda} = 3:
					set {_color} to last color of {klejnot}'s name
					if {language::%player%} = "English":
						set {_nagroda} to " %{_color}%%{_multiplier}*40% %{klejnot}'s name%"
					else:
						set {_nagroda} to " %{_color}%%{_multiplier}*40% %{klejnot}'s name%ów"
				if {_nagroda} = 4:
					set {_color} to last color of {klejnot}'s name
					if {language::%player%} = "English":
						set {_nagroda} to " %{_color}%%{_multiplier}*30% %{klejnot}'s name%"
					else:
						set {_nagroda} to " %{_color}%%{_multiplier}*30% %{klejnot}'s name%ów"
