function minenew(p: player, t: text, b: block):
	if {_t} = "deepslate":
		set {_blok} to 2
		add 1 to {deepslate::%{_p}%}
	else if {_t} = "stone":
		set {_blok} to 1
		add 1 to {stone::%{_p}%}
	if {event} = "Winter Delight":
		if {winterDelightPassLEVEL::%{_p}%} >= 0:
			getMission({_p}, "Festive Mining", "Świąteczne Kopanie", {_blok})
	loop {_blok} times:
		achievement("stone", {_p}, 100, "osiagniecie.100kam", "Wykop 100 kamienia", "R(o)ocky", "Mine 100 stone", "R(o)ocky")
		achievement("stone", {_p}, 1000, "osiagniecie.1000kam", "Wykop 1000 kamienia", "Adept Kamienia", "Mine 1000 stone", "Stone Adept")
		achievement("stone", {_p}, 10000, "osiagniecie.10000kam", "Wykop 10000 kamienia", "Górnik", "Mine 10000 stone", "Miner")
		achievement("stone", {_p}, 100000, "osiagniecie.100000kam", "Wykop 100000 kamienia", "Ekspert Kopania", "Mine 100000 stone", "Mining Expert")
		achievement("stone", {_p}, 1000000, "osiagniecie.1000000kam", "Wykop 1000000 kamienia", "Milion: Kopanie", "Mine 1000000 stone", "Million: Mining")
		addPlayerXP({_p}, 1)
		addMiningXP({_p}, 1)
		add 1 to {expkopanie::%{_p}%}
		add 1 to {weeklyStoneMined::%{_p}%}
		add 1 to {totalStoneMined::%{_p}%}
		set {_data} to data()
		chance of 1%:
			add 1 to {skrzynka::%{_p}%}
			set {_otxp} to 6
			set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
			add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
			addMiningXP({_p}, {_otxp})
			if {_otxpAfterMultiplier} > {_otxp}:
				set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
			else:
				set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"
			if {language::%{_p}%} = "English":
				drop {skrzynka} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a &9Chest&7! %{_xpmsg}%" to {_p}
			else:
				drop {skrzynka} at {_b}
				send "&8[&6Earthcraft&8] &7Gratulacje! Wykopałeś/aś &9Skrzynkę&7! %{_xpmsg}%" to {_p}
			achievement("skrzynka", {_p}, 100, "osiagniecie.100skrzynek", "Wykop 100 Skrzynek", "Skrzynkoznajdywacz", "Find 100 Chests", "Chest Finder")
			achievement("skrzynka", {_p}, 1000, "osiagniecie.1000skrzynek", "Wykop 1000 Skrzynek", "Miłośnik Skrzynek", "Find 1000 Chests", "Chest Lover")
		chance of 0.3%:
			add 1 to {skrzynkaluksus::%{_p}%}
			set {_otxp} to 14
			set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
			add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
			addMiningXP({_p}, {_otxp})
			if {_otxpAfterMultiplier} > {_otxp}:
				set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
			else:
				set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"
			if {language::%{_p}%} = "English":
				drop {skrzynkaluksusowa} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a &9Luxurious Chest&7! %{_xpmsg}%" to {_p}
			else:
				drop {skrzynkaluksusowa} at {_b}
				send "&8[&6Earthcraft&8] &7Gratulacje! Wykopałeś/aś &9Luksusową Skrzynkę&7! %{_xpmsg}%" to {_p}
			achievement("skrzynkaluksus", {_p}, 100, "osiagniecie.100luksusskrzynek", "Wykop 100 Luksusowych Skrzynek", "Znajdywacz Luksusowych Skrzynek", "Find 100 Luxurious Chests", "Luxurious Chest Finder")
			achievement("skrzynkaluksus", {_p}, 1000, "osiagniecie.1000luksusskrzynek", "Wykop 1000 Luksusowych Skrzynek", "Miłośnik Luksusowych Skrzynek", "Find 1000 Luxurious Chests", "Luxurious Chest Lover")
		
		if {diademdrop::%{_p}%} = "esencje":
			set {_che} to 2 + ({bonusesencja::%{_p}%}*4)
		else:
			set {_che} to 0.5 + {bonusesencja::%{_p}%}
		if int tag "Enchant;Essence Reaver" of custom nbt of {_p}'s tool = 1:
			set {_che} to {_che}*1.04
		chance of {_che}%:
			set {_essence} to a random element of {esencje::*}
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			set {_essenceName} to unformatted (name of {_essence} in lowercase)
			replace all " " in {_essenceName} with ""
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Essenceful", "Esencyjność", {_damount})
			loop {_damount} times:
				drop {_essence} at {_b}
				add 1 to {wykopaneesencje::%{_p}%}
				achievement("wykopaneesencje", {_p}, 100, "osiagniecie.100es", "Wykop 100 Esencji", "Żywiołak", "Find 100 Essences when mining", "Elemental")
				achievement("wykopaneesencje", {_p}, 1000, "osiagniecie.100es", "Wykop 100 Esencji", "Jeden z Żywiołami", "Find 1000 Essences when mining", "One with the Elements")
				add 1 to {%{_essenceName}%::%{_p}%}
				set {_otxp} to 12
				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})
				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"
				send "&8[&6Earthcraft&8] &7Congratulations! You found a %name of {_essence}%&7! %{_xpmsg}%" to {_p}

		set {_chanceSapphire} to 2
		chance of {_chanceSapphire}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {sapphire::%{_p}%}

				if {sapphire::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Sapphire.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Sapphire: 100", "Find 100 Sapphires while mining.")
				else if {sapphire::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Sapphire.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Sapphire: 1000", "Find 1000 Sapphires while mining.")

				set {_otxp} to 2.5

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {szafir} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##025b95>Sapphire&7! %{_xpmsg}%" to {_p}

		set {_chanceKlejnot} to 1 #+ {bonusklejnot::%{_p}%} + {rangaklejnot::%{_p}%}
		#if int tag "Enchant;Treasure Seeker" of custom nbt of {_p}'s tool = 1:
			#set {_chk} to {_chk}*1.05
			
		chance of {_chanceKlejnot}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Klejnots for Stavros", "Klejnoty dla Stavrosa", {_damount})
			loop {_damount} times:
				add 1 to {klejnot::%{_p}%}
				#if {klejnot::%{_p}%} = 300:
				#	set {rangaklejnot::%{_p}%} to 0.5
				#	broadcast "&8[&6Earthcraft&8] &7Gracz &6%{_p}% &7osiągnął &6rangę 1 &7[&e&l✰&8&l✰✰&7] &7na <##b5e550>Klejnocie&7!"
				#	soundRankup({_p})
				#if {klejnot::%{_p}%} = 600:
				#	set {rangaklejnot::%{_p}%} to 1
				#	broadcast "&8[&6Earthcraft&8] &7Gracz &6%{_p}% &7osiągnął &6rangę 2 &7[&e&l✰✰&8&l✰&7] &7na <##b5e550>Klejnocie&7!"
				#	soundRankup({_p})
				#if {klejnot::%{_p}%} = 1200:
				#	set {rangaklejnot::%{_p}%} to 1.5
				#	broadcast "&8[&6Earthcraft&8] &7Gracz &6%{_p}% &7osiągnął &6rangę 3 &7[&e&l✰✰✰&7] &7na <##b5e550>Klejnocie&7!"
				#	soundRankup({_p})

				add 1 to {klejnot::%{_p}%}

				if {klejnot::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Klejnot.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Klejnot: 100", "Find 100 Klejnots while mining.")
				else if {klejnot::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Klejnot.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Klejnot: 1000", "Find 1000 Klejnots while mining.")

				set {_otxp} to 5

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {klejnot} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##b5e550>Klejnot&7! %{_xpmsg}%" to {_p}

		set {_chanceRuby} to 0.5
		chance of {_chanceRuby}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {ruby::%{_p}%}

				if {ruby::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Ruby.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Ruby: 100", "Find 100 Rubies while mining.")
				else if {ruby::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Ruby.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Ruby: 1000", "Find 1000 Rubies while mining.")

				set {_otxp} to 10

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {rubin} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##ff0000>Ruby&7! %{_xpmsg}%" to {_p}

		set {_chanceAmethyst} to 0.25
		chance of {_chanceAmethyst}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {amethyst::%{_p}%}

				if {amethyst::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Amethyst.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Amethyst: 100", "Find 100 Amethysts while mining.")
				else if {amethyst::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Amethyst.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Amethyst: 1000", "Find 1000 Amethysts while mining.")

				set {_otxp} to 20

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {ametyst} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##ae78d0>Amethyst&7! %{_xpmsg}%" to {_p}

		set {_chanceTopaz} to 0.125
		chance of {_chanceTopaz}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {topaz::%{_p}%}

				if {topaz::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Topaz.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Topaz: 100", "Find 100 Topazes while mining.")
				else if {topaz::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Topaz.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Topaz: 1000", "Find 1000 Topazes while mining.")

				set {_otxp} to 40

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {topaz} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##ffe505>Topaz&7! %{_xpmsg}%" to {_p}

		set {_chanceAquamarine} to 0.0625
		chance of {_chanceAquamarine}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {aquamarine::%{_p}%}

				if {aquamarine::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Aquamarine.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Aquamarine: 100", "Find 100 Aquamarinees while mining.")
				else if {aquamarine::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Aquamarine.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Aquamarine: 1000", "Find 1000 Aquamarinees while mining.")

				set {_otxp} to 80

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {akwamaryn} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##11d7d8>Aquamarine&7! %{_xpmsg}%" to {_p}

		set {_chancePeridot} to 0.03125
		chance of {_chancePeridot}%:
			set {_damount} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_damount} to 2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Christmas Baubles", "Świąteczne Ozdoby", {_damount})
			loop {_damount} times:
				add 1 to {peridot::%{_p}%}

				if {peridot::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Peridot.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Peridot: 100", "Find 100 Peridotes while mining.")
				else if {peridot::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Peridot.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Peridot: 1000", "Find 1000 Peridotes while mining.")

				set {_otxp} to 160

				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxpAfterMultiplier} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})

				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "&7(+%{_otxpAfterMultiplier}% &7XP)"

				drop {akwamaryn} at {_b}
				send "&8[&6Earthcraft&8] &7Congratulations! You found a <##c0ff02>Peridot&7! %{_xpmsg}%" to {_p}
		
		set {_chanceAmber} to 0.2
		if int tag "Enchant;Archeologist" of custom nbt of {_p}'s tool = 1:
			set {_chanceAmber} to {_chanceAmber}*1.03
		chance of {_chanceAmber}%:
			set {_preznal} to 1
			if {podwojnydrop::%{_p}%} = true:
				set {_preznal} to {_preznal}*2
			if {gornictwo::preznal::%{_p}%} is true:
				chance of 20%:
					set {_preznal} to {_preznal}*2
			#if {winterDelightPassLEVEL::%{_p}%} >= 0:
			#	getMission({_p}, "Magnus's Request", "Prośba Magnusa", {_preznal})
			loop {_preznal} times:
				add 1 to {amber::%{_p}%}

				if {amber::%{_p}%} = 100:
					set {_yamlValue} to "Achievement.Mining.Amber.100"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Amber: 100", "Find 100 Ambers while mining.")
				else if {amber::%{_p}%} = 1000:
					set {_yamlValue} to "Achievement.Mining.Amber.1000"
					achievementYaml({_p}, "plugins/Skript/playerdata/achievements", "Achievements", {_yamlValue}, "Amber: 1000", "Find 1000 Ambers while mining.")

				set {_otxp} to 30
				set {_otxpAfterMultiplier} to addPlayerXP({_p}, {_otxp}, 1, "mining")
				add {_otxp} to {expkopanie::%{_p}%}
				addMiningXP({_p}, {_otxp})
				drop {bursztyn} at {_b}
				
			if {_preznal} > 1:
				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "(<##ffab24>x%{_preznal}%&7) (+%{_otxpAfterMultiplier}*{_preznal}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "(<##ffab24>x%{_preznal}%&7) (+%{_otxpAfterMultiplier}*{_preznal}% &7XP)"
			else:
				if {_otxpAfterMultiplier} > {_otxp}:
					set {_xpmsg} to "(+%{_otxpAfterMultiplier}*{_preznal}% &7XP) (&fx%{_otxpAfterMultiplier}/{_otxp}%&7)"
				else:
					set {_xpmsg} to "(+%{_otxpAfterMultiplier}*{_preznal}% &7XP)"
			send "&8[&6Earthcraft&8] &7Congratulations! You found an <##ffab24>Amber&7! %{_xpmsg}%" to {_p}
		#DIAMONDSHARD
