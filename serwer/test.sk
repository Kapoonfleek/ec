function IcicleDistance(e: entity, target: entity, p: player):
	while distance between {_e} and {_target} >= 2:
		if {_target} is alive:
			push {_e} (direction from {_p} to {_target}) with speed 1
			wait 2 ticks
		else:
			stop loop
	kill {_e}
