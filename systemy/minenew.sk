						send "&8[&6Earthcraft&8] &7Otrzymujesz &6%{poziom::%{_p}%}% %{stardust}'s name%u &7za osiągnięcie nowego poziomu&7!" to {_p}

			send "Gracz %{_p}% właśnie osiągnął %{poziom::%{_p}%}% poziom!" to console
			log "%{_data}% Gracz %{_p}% właśnie osiągnął %{poziom::%{_p}%}% poziom!" to "logi/poziomy.log"
			execute console command "discord broadcast ##earthcraft %{_data}% | %{_p}% reached level %{poziom::%{_p}%}%!"
			#set the {_p}'s tab name to "&8[<##9b21ad>&l%{poziom::%{_p}%}%&f&8] &f%{_p}%"
			#set the {_p}'s tab name to "&8[<##504cc2>&l%{poziom::%{_p}%}%&f&l&8] &f%{_p}%"
