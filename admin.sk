command /adminator:
	trigger:
		loop all entities in radius 3 around player:
			if loop-entity is armor stand:
				kill loop-entity
		tornado(player, 3, 1, 4, 1)
		broadcast "%{_l::*}%"
		show 1 blue dust at {_l::*}

