	else if clicked player's name = "Ciscra":
		if line 19 of lore of player's tool contains "&8Id: ":
			cancel event
			set metadata tag "Constellation" of player to chest inventory with 3 rows named "Constellation"
			set {_constellation} to Constellation(player, false)
			if {_constellation} is not set:
				set {_constellation} to 0
			set {_constellationName} to name of slot 16 of metadata tag "kartagui" of player
			set {_lore} to (join lore of slot 16 of metadata tag "kartagui" of player by nl)
			set slot 4 of metadata tag "Constellation" of player to nether star named {_constellationName} with lore {_lore}
			loop slots integers between 10 and 15 of metadata tag "kartagui" of player:
				if {_constellation} >= (index of loop-slot) - 9:
					add "%last color of name of player's tool%%uncolored name of loop-value%" to {_constList::*}
				else:
					add "&8%uncolored name of loop-value%" to {_constList::*}
			set {_constList} to join {_constList::*} by "%nl%&7- "
			set slot 11 of metadata tag "Constellation" of player to white glass pane named "&fCurrent Constellation" with lore "&7- %{_constList}%"
			if {_constellation} < 6:
				set slot 15 of metadata tag "Constellation" of player to white glass pane named "&fUnlock Constellation" with lore "&7Click here to exchange a copy of held%nl%&7card and increase the constellation."
			else:
				set slot 15 of metadata tag "Constellation" of player to white glass pane named "&fAll Constellations Unlocked" with lore "&7This card's full potential has been unlocked."
			open metadata tag "Constellation" of player to player
			loop 4 times:
				set {_pitch} to 0.5 + (loop-number*0.2)
				play sound "entity.experience_orb.pickup" with volume 3 and pitch {_pitch} to player
				wait 3 ticks
				if loop-number = 2:
					wait 3 ticks
