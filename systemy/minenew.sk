	add 1 to {blocksplaced::%player%}
	if cooldown "place%player%" is not finished:
		stop
	else:
		create cooldown "place%player%" for 0.25 seconds
		add 1 to {exp::%player%}
		UpdateScoreboard(player)
function KolorekScoreboard(p: player) :: string:
	if {kolorek::%{_p}%} is set:
		set {_kolorek} to {kolorek::%{_p}%}
	else:
		if {poziom::%{_p}%} is smaller than 10:
			set {_kolorek} to "&6"
		if {poziom::%{_p}%} is smaller than 20:
			if {poziom::%{_p}%} is bigger than 9:
				set {_kolorek} to "&b"
		if {poziom::%{_p}%} is smaller than 30:
			if {poziom::%{_p}%} is bigger than 19:
				set {_kolorek} to "&c"
		if {poziom::%{_p}%} is smaller than 40:
			if {poziom::%{_p}%} is bigger than 29:
				set {_kolorek} to "&d"
		if {poziom::%{_p}%} is smaller than 50:
			if {poziom::%{_p}%} is bigger than 39:
				set {_kolorek} to "&a"
		if {poziom::%{_p}%} is smaller than 60:
			if {poziom::%{_p}%} is bigger than 49:
				set {_kolorek} to "&9"
		if {poziom::%{_p}%} is smaller than 70:
			if {poziom::%{_p}%} is bigger than 59:
				set {_kolorek} to "&e"
		if {poziom::%{_p}%} is smaller than 80:
			if {poziom::%{_p}%} is bigger than 69:
				set {_kolorek} to "&5"
		if {poziom::%{_p}%} is smaller than 90:
			if {poziom::%{_p}%} is bigger than 79:
				set {_kolorek} to "&3"
		if {poziom::%{_p}%} is smaller than 100:
			if {poziom::%{_p}%} is bigger than 89:
				set {_kolorek} to "&4"
	return {_kolorek}

function UpdateScoreboard(p: player):
	if {engagedc::%{_p}%} is not set:
		if {scorepl::%{_p}%} is not false:
			set {_kolorek} to KolorekScoreboard({_p})
			setup skoreboard for {_p}
			set {_e} to uncolored {kartaname::%{_p}%}
			set title of skoreboard {_p} to "&f&l«-───▬ %{_kolorek}%&lEC &f&l▬───-»"
			set slot 13 of skoreboard {_p} to ""
			if {language::%{_p}%} = "Polski":
				set slot 12 of skoreboard {_p} to " %{_kolorek}%&l* &f&lGRACZ:"
				set slot 11 of skoreboard {_p} to " &fKarta: %{_kolorek}%%{_e}%"
				set slot 10 of skoreboard {_p} to " &fWykopany kamień: %{_kolorek}%%{stone::%{_p}%}%"
				set slot 9 of skoreboard {_p} to " &fPoziom: %{_kolorek}%%{poziom::%{_p}%}%"
			else:
				set slot 12 of skoreboard {_p} to " %{_kolorek}%&l* &f&lPLAYER:"
				set slot 11 of skoreboard {_p} to " &fCard: %{_kolorek}%%{_e}%"
				set slot 10 of skoreboard {_p} to " &fStone Mined: %{_kolorek}%%{stone::%{_p}%}%"
				set slot 9 of skoreboard {_p} to " &fLevel: %{_kolorek}%%{poziom::%{_p}%}%"
			set slot 8 of skoreboard {_p} to " &fXP: %{_kolorek}%%{exp::%{_p}%}%&8/%{_kolorek}%%100*{poziom::%{_p}%}%"
			set {_c} to rounded down {exp::%{_p}%}/({poziom::%{_p}%}*100)*10
			if {_c} = 0:
