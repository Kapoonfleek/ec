function miningProfession(p: player):
	if {poziomgornictwo::%{_p}%} < 50:
		if {expgornictwo::%{_p}%} is bigger than or equal to 650 + (650*{poziomgornictwo::%{_p}%}-1):
			remove (650 + (650*{poziomgornictwo::%{_p}%}-1)) from {expgornictwo::%{_p}%}
			add 1 to {poziomgornictwo::%{_p}%}
			add 1 to {orgpoziomgornictwo::%{_p}%}
