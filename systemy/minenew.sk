function playerCheckLevelUp(p: player):
	if {exp::%{_p}%} >= (100*{poziom::%{_p}%}):
		remove (100*{poziom::%{_p}%}) from {exp::%{_p}%}
		add 1 to {poziom::%{_p}%}
		add {poziom::%{_p}%} to {stardust::%{_p}%}
		add {poziom::%{_p}%} to {totalstardust::%{_p}%}
		add 5*{poziom::%{_p}%} to {cosmicflux::%{_p}%}
		add 5*{poziom::%{_p}%} to {totalcosmicflux::%{_p}%}
		set {dziennylimit::stardust::%{_p}%} to {poziom::%{_p}%}
		add 1 to {orgpoziom::%{_p}%}
		set {_data} to data()
		if {winterDelightPassLEVEL::%{_p}%} >= 0:
			getMission({_p}, "Progress", "Progres", 1)
		achievement("poziom", {_p}, 10, "osiagniecie.10lvl", "Osiągnij dziesiąty poziom", "10 LVL", "Achieve level ten", "10 LVL")
		achievement("poziom", {_p}, 50, "osiagniecie.50lvl", "Osiągnij pięćdziesiąty poziom", "50 LVL", "Achieve level fifty", "50 LVL")
		achievement("poziom", {_p}, 100, "osiagniecie.100lvl", "Osiągnij setny poziom", "100 LVL", "Achieve level one hundred", "100 LVL")
		achievement("poziom", {_p}, 200, "osiagniecie.200lvl", "Osiągnij dwusetny poziom", "200 LVL", "Achieve level two hundred", "200 LVL")
		achievement("poziom", {_p}, 300, "osiagniecie.300lvl", "Osiągnij trzysetny poziom", "300 LVL", "Achieve level three hundred", "300 LVL")
		achievement("poziom", {_p}, 500, "osiagniecie.500lvl", "Osiągnij pięćsetny poziom", "500 LVL", "Achieve level five hundred", "500 LVL")
		achievement("poziom", {_p}, 1000, "osiagniecie.1000lvl", "Osiągnij tysięczny poziom", "1000 LVL", "Achieve level one thousand", "1000 LVL")
		if {poziom::%{_p}%} = 10:
			set {_colour} to "#55ffff"
		if {poziom::%{_p}%} = 20:
			set {_colour} to "#ff5555"
		if {poziom::%{_p}%} = 30:
			set {_colour} to "#FF55FF"
		if {poziom::%{_p}%} = 40:
			set {_colour} to "#55FF55"
		if {poziom::%{_p}%} = 50:
			set {_colour} to "#5555ff"
		if {poziom::%{_p}%} = 60:
			set {_colour} to "#FFFF55"
		if {poziom::%{_p}%} = 70:
			set {_colour} to "#AA00AA"
		if {poziom::%{_p}%} = 80:
			set {_colour} to "#00AAAA"
		if {poziom::%{_p}%} = 90:
			set {_colour} to "#aa0000"
		set {kolorek1::%{_p}%} to "<%{_colour}%>"
		if {poziom::%{_p}%} is divisible by 50:
			add 1 to {nagrodalvl::karta::%{_p}%}
			send formatted "&8[&6Earthcraft&8] &7Congratulations! You may choose a new card for reaching level &6%{poziom::%{_p}%}%!" to {_p}
			send formatted "&8[&6Earthcraft&8] &7Click <command:/reward claim><tooltip:&fClick to choose a card>&6[here]<tooltip:> &7or write &6/reward claim &7in chat to claim your reward." to {_p}
		send formatted "&8[&6Earthcraft&8] &7Congratulations! You receive a &cStar Shard &7for reaching level &6%{poziom::%{_p}%}%!" to {_p}
		giveItem({_p}, 1 of {odlamekgwiazdy})

		play sound "entity.player.levelup" with volume 3 and pitch 2 to {_p}
		if {poziom::%{_p}%} is set:
			loop all players:
				if {language::%loop-player%} = "English":
					if loop-player is not {_p}:
						send "&8[&6Earthcraft&8] &6%{_p}% &7reached level &6%{poziom::%{_p}%}%&7!" to loop-player
					else:
						send "&8[&6Earthcraft&8] &7Congratulations! You reached level &6%{poziom::%{_p}%}% &7lvl!" to {_p}
						send "&8[&6Earthcraft&8] &7You receive &6%5*{poziom::%{_p}%}% <##ffe17a>Cosmic Flux &7as a reward for reaching a new level!" to {_p}
						send "&8[&6Earthcraft&8] &7You receive &6%{poziom::%{_p}%}% %{stardust}'s name% &7as a reward for reaching a new level!" to {_p}
				else:
					if loop-player is not {_p}:
						send "&8[&6Earthcraft&8] &7Gracz &6%{_p}% &7osiągnął &6%{poziom::%{_p}%}% &7lvl!" to loop-player
					else:
