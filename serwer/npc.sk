on join:
	if {completionreward::%player%::*} is set:
		wait 6 seconds
		play sound "block.note_block.pling" with volume 3 and pitch 1.6 to player
		if {language::%player%} = "English":
			send "&e> You have %size of {completionreward::%player%::*}% unclaimed completion rewards."
		else:
			send "&e> Masz %size of {completionreward::%player%::*}% nieodebranych nagród za skompletowanie kolekcji."
	else:
		if {secretwonderscomplete::%player%} is not true:
			loop 14 times:
				if {carddrop::sw::%player%::%loop-number%} is set:
					add 1 to {_complete}
			if {_complete} = 14:
				add "Secret Wonders" to {completionreward::%player%::*}
				set {secretwonderscomplete::%player%} to true
