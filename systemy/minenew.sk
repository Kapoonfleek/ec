on rightclick:
	if event-block is tagged with block tag "mining_blocks":
		if player's tool is tagged with minecraft tag "pickaxes":
			if region at player is not set:
				if player is sneaking:
					if {umkilof::%player%} = "PomocnaDłoń":
						if cooldown "HelpingHand%player%" is finished:
							create cooldown "HelpingHand%player%" for 10 seconds
							set {_block} to "%type of clicked block%"
							replace all " block" in {_block} with ""
							minenew(player, {_block}, clicked block)
							break target block naturally using player's held item
