					set {_repnextlvl} to {_repnext}
				set {_10procent} to 0.1*{_repexp}
				set {_kwadraciki} to {reputacjaexp::%{_zleceniodawca}%::%player%}/{_10procent}
				set {_stata} to "&7■■■■■■■■■■"
