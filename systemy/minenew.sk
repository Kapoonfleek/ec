variables:
	#{kolorek1::%{_p}%} = "&6"
	{poziom::%player%} = 0
	{poziomgornictwo::%player%} = 0

on entity breed:
	add 1 to {%event-entity%::%breeder%}
	set {_xp} to addPlayerXP(breeder, 3)
	add {_xp} to {expbreeding::%player%}
	UpdateScoreboard(breeder)

on join:
	UpdateScoreboard(player)
on rightclick:
	if "%region at player%" does not contain "spawn" or "oldspawn":
		if player is not sneaking:
			if clicked block is sweet berry bush:
				if block data tag "age" of clicked block = 3:
					add 2 to {berry::%player%}
					set {_xp} to addPlayerXP(player, 0.4)
					add {_xp} to {expharvest::%player%}
					UpdateScoreboard(player)
				if block data tag "age" of clicked block = 2:
					add 1 to {berry::%player%}
					set {_xp} to addPlayerXP(player, 0.2)
					add {_xp} to {expharvest::%player%}
					UpdateScoreboard(player)
on rightclick:
	if player's tool is bone meal:
		if block data tag "age" of clicked block < 7:
			if "%region at player%" does not contain "spawn" or "oldspawn":
				add 1 to {bonemeal::%player%}
				set {_xp} to addPlayerXP(player, 0.3)
				add {_xp} to {expbonemeal::%player%}
				UpdateScoreboard(player)

on mine:
	if player's gamemode is survival:
		if region at event-location is not set:
			if event-block = cobblestone:
				add 1 to {cobblestone::%player%}
				set {_xp} to addPlayerXP(player, 0.5)
				add {_xp} to {expkopanie::%player%}
			else if event-block = nether gold ore:
				add 1 to {nethergold::%player%}
				set {_xp} to addPlayerXP(player, 1.75)
				add {_xp} to {expkopanie::%player%}
			else if event-block = obsidian:
				add 1 to {obsidian::%player%}
				set {_xp} to addPlayerXP(player, 4)
				add {_xp} to {expkopanie::%player%}
			else if event-block = dirt:
				add 1 to {dirt::%player%}
				set {_xp} to addPlayerXP(player, 0.5)
				add {_xp} to {expdigging::%player%}
			else if event-block = end stone:
				add 1 to {endstone::%player%}
				set {_xp} to addPlayerXP(player, 1)
				add {_xp} to {expkopanie::%player%}
			else if event-block = gravel:
				add 1 to {gravel::%player%}
				set {_xp} to addPlayerXP(player, 0.5)
				add {_xp} to {expdigging::%player%}
			else if event-block = sand:
				add 1 to {sand::%player%}
				set {_xp} to addPlayerXP(player, 0.5)
				add {_xp} to {expdigging::%player%}
			else if event-block = netherrack:
				add 1 to {netherrack::%player%}
				set {_xp} to addPlayerXP(player, 0.1)
				add {_xp} to {expkopanie::%player%}
			else if event-block is beetroots:
				if age of event-block = 3:
					add 1 to {beetroot::%player%}
					set {_xp} to addPlayerXP(player, 0.5)
					add {_xp} to {expharvest::%player%}
			else if event-block is carrots:
				if age of event-block = 7:
					add 1 to {carrot::%player%}
					set {_xp} to addPlayerXP(player, 0.5)
					add {_xp} to {expharvest::%player%}
			else if event-block is wheat:
				if age of event-block = 7:
					add 1 to {wheat::%player%}
					set {_xp} to addPlayerXP(player, 0.5)
					add {_xp} to {expharvest::%player%}
			else if event-block is potatoes:
				if age of event-block = 7:
					add 1 to {potato::%player%}
					set {_xp} to addPlayerXP(player, 0.5)
					add {_xp} to {expharvest::%player%}
			else if event-block is tagged with tag "wood":
				add 1 to {%event-block%::%player%}
				set {_xp} to addPlayerXP(player, 1)
				add {_xp} to {expwood::%player%}
			else if event-block is tagged with tag "logs":
				add 1 to {%event-block%::%player%}
				set {_xp} to addPlayerXP(player, 1)
				add {_xp} to {expwood::%player%}
			else if event-block is tagged with tag "stripped_logs":
				add 1 to {%event-block%::%player%}
				set {_xp} to addPlayerXP(player, 1)
				add {_xp} to {expwood::%player%}
			else if event-block is tagged with tag "leaves":
				add 1 to {%event-block%::%player%}
				set {_xp} to addPlayerXP(player, 0.25)
				add {_xp} to {expleaves::%player%}
			UpdateScoreboard(player)
	

on break of melon:
	if region at event-location is not set:
		add 1 to {melon::%player%}
		set {_xp} to addPlayerXP(player, 3)
		add {_xp} to {expharvest::%player%}
		UpdateScoreboard(player)
on break of pumpkin:
	if region at event-location is not set:
		add 1 to {pumpkin::%player%}
		set {_xp} to addPlayerXP(player, 3)
		add {_xp} to {expharvest::%player%}
		UpdateScoreboard(player)
		
on death:
	if attacker is a player:
		if {passivemobs::*} contains "%type of victim%":
			if boolean tag "ECMob" of custom nbt of victim is not set:
				set {_xp} to addPlayerXP(attacker, 2)
				add {_xp} to {%type of victim%::%attacker%}
				UpdateScoreboard(victim)
on block place:
		
	if event-block is tagged with block tag "glass_panes":
		if name of player's tool is set:
			cancel event

	else if event-block is tagged with paper tag "ores":
		set {placed::%event-block%} to true
		if player's gamemode is survival:
			if (player's offhand item) is tagged with paper tag "ores":
				cancel event

	if event is not cancelled:
		set {_bannedBlocks::*} to stone and deepslate and player head
		if {_bannedBlocks::*} does not contain (1 of player's offhand item):
			add 1 to {blocksplaced::%player%}
			if cooldown "place%player%" is not finished:
				stop
			else:
				create cooldown "place%player%" for 0.25 seconds
				set {_xp} to addPlayerXP(player, 1)
				add {_xp} to {expbudowanie::%player%}
				UpdateScoreboard(player)
				#if {winterDelightPassLEVEL::%player%} >= 0:
					#getMission(player, "Building the World", "Budowanie Świata", 1)
			
function KolorekScoreboard(p: player) :: string:
	if {poziom::%{_p}%} >= 100:
		set {_kolorek} to formatted {kolorek1::%{_p}%}
	else if {poziom::%{_p}%} is smaller than 10:
		set {_kolorek} to "&6"
	else if {poziom::%{_p}%} is smaller than 20:
		if {poziom::%{_p}%} is bigger than 9:
			set {_kolorek} to "&b"
	else if {poziom::%{_p}%} is smaller than 30:
		if {poziom::%{_p}%} is bigger than 19:
			set {_kolorek} to "&c"
	else if {poziom::%{_p}%} is smaller than 40:
		if {poziom::%{_p}%} is bigger than 29:
			set {_kolorek} to "&d"
	else if {poziom::%{_p}%} is smaller than 50:
		if {poziom::%{_p}%} is bigger than 39:
			set {_kolorek} to "&a"
	else if {poziom::%{_p}%} is smaller than 60:
		if {poziom::%{_p}%} is bigger than 49:
			set {_kolorek} to "&9"
	else if {poziom::%{_p}%} is smaller than 70:
		if {poziom::%{_p}%} is bigger than 59:
			set {_kolorek} to "&e"
	else if {poziom::%{_p}%} is smaller than 80:
		if {poziom::%{_p}%} is bigger than 69:
			set {_kolorek} to "&5"
	else if {poziom::%{_p}%} is smaller than 90:
		if {poziom::%{_p}%} is bigger than 79:
			set {_kolorek} to "&3"
	else if {poziom::%{_p}%} is smaller than 100:
		if {poziom::%{_p}%} is bigger than 89:
			set {_kolorek} to "&4"
	return {_kolorek}

function UpdateScoreboard(p: player):
	if metadata value "ECEngaged" of {_p} is not set:
		if metadata value "DungeonInstance" of {_p} is not set:
			if metadata value "IslandExpeditionInstance" of {_p} is not set:
				if {scorepl::%{_p}%} is not false:
					set {_kolorek} to KolorekScoreboard({_p})
					if title of {_p}'s fastboard is set:
						if title of {_p}'s fastboard != "&f&l«-───▬ %{_kolorek}%&lEC &f&l▬───-»":
							clear {_p}'s fastboard
					set {_e} to uncolored {kartaname::%{_p}%}
					set title of {_p}'s fastboard to "&f&l«-───▬ %{_kolorek}%&lEC &f&l▬───-»"
					set line 2 of {_p}'s fastboard to ""
					set line 3 of {_p}'s fastboard to " %{_kolorek}%&l* &f&lPLAYER:"
					set line 4 of {_p}'s fastboard to " &fCard: %{_kolorek}%%{_e}%"
					set line 5 of {_p}'s fastboard to " &fStone Mined: %{_kolorek}%%{stone::%{_p}%} + {deepslate::%{_p}%}*2%"
					set line 6 of {_p}'s fastboard to " &fLevel: %{_kolorek}%%{poziom::%{_p}%}%"
					set line 7 of {_p}'s fastboard to " &fXP: %{_kolorek}%%{exp::%{_p}%}%&8/%{_kolorek}%%100*{poziom::%{_p}%}%"
					set {_c} to rounded down {exp::%{_p}%}/({poziom::%{_p}%}*100)*10
					if {_c} = 0:
						set line 8 of {_p}'s fastboard to "    &8&l■■■■■■■■■■"
					if {_c} = 1:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■&8&l■■■■■■■■■"
