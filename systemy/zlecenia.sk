				loop {_kwadraciki} times:
					set {_f} to character at 2 + loop-number-2 in {_stata}
					replace all "■" in {_f} with "&a■&7"
					set {_statas::%loop-number-2 + 2%} to {_f}
				set {_stata} to join {_statas::*} by ""
				set {_procent} to ({reputacjaexp::%{_zleceniodawca}%::%player%}/{_10procent})*100
				add "&7Reputacja" and "&f%{reputacja::%{_zleceniodawca}%::%player%}% %{_stata}% &f%{_repnext}% &7(%{_procent}%%%)" and "    &7%{reputacjaexp::%{_zleceniodawca}%::%player%}%/%{_repexp}%" and " " and "&7Nagroda za kolejny poziom:" and "&b%{nagroda::rep::%{_repnextlvl}%::%{_zleceniodawca}%}%" to lore of slot {_slot} of ({@zlecenia})
				add slot {_slot} of ({@zlecenia}) to {zlecenia::%player%::*}
				clear {_profesja}
				clear {_nagroda}
				clear {_repreward}
