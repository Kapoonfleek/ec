		set slot 24 of metadata tag "Teams" of player to light blue wool named "&bTeam Aqua" with lore "&fTeam Aqua needs you!%nl% %nl%&7&oDescription:%nl%&7&oAdaptible and investigative, Team Aqua has a history%nl%&7&oof finding bases effectively, acquiring and collecting%nl%&7&oevidence and secret items, as well as quickly unfolding%nl%&7&othe opposing team's plans. They are especially known for%nl%&7&onever letting their guard down and being exceptionally%nl%&7&ogood at protecting what must be hidden from the public eye.%nl% %nl%&7Members:%nl%%{_membersAqua}%"
		
		set slot 31 of metadata tag "Teams" of player to white wool named "&fImpartial" with lore "&7Don't feel like joining any team?%nl%&7You can choose to remain impartial%nl%&7by clicking on this panel.%nl% %nl%&fClick here if you want to remain impartial."
		if {team::%player%} is not set:
			add "%nl%&fClick here to join Team Poison." to lore of slot 20 of metadata tag "Teams" of player	
			add "%nl%&fClick here to join Team Aqua." to lore of slot 24 of metadata tag "Teams" of player	
		open metadata tag "Teams" of player to player

on inventory click:
	if player's current inventory = metadata tag "Teams" of player:
		cancel event
	else if event-inventory = metadata tag "Constellation" of player:
		cancel event
		if index of clicked slot = 15:
			if name of clicked slot != "&fAll Constellations Unlocked":
				teleport the player to player's location
				set metadata tag "ConstellationAdd" of player to chest inventory with 3 rows named "Constellation"
				set {_slotIndex} to 0
				loop all items in the player's inventory:
					if line 19 of lore of loop-item contains "&8Id: ":
						if line 19 of lore of loop-item != line 19 of lore of player's tool:
							if name of loop-item = name of player's tool:
								set slot {_slotIndex} of metadata tag "ConstellationAdd" of player to loop-item
								add "%nl%&c&l(&4&l!&c&l) &f&oClick here to exchange this item." to lore of slot {_slotIndex} of metadata tag "ConstellationAdd" of player
								add 1 to {_slotIndex}
				if {_slotIndex} > 0:
					open metadata tag "ConstellationAdd" of player to player
				else:
					play sound "entity.villager.no" with volume 5 and pitch 0.62 to player
					play sound "block.glass.break" with volume 5 and pitch 1.176 to player
					if {language::%player%} = "English":
						send "&c&oThere are no identical cards in your inventory." to player
					else:
						send "&c&oNie ma identycznych kart w twoim ekwipunku." to player
			else:
				play sound "entity.villager.no" with volume 5 and pitch 0.62 to player
				play sound "block.glass.break" with volume 5 and pitch 1.176 to player
				if {language::%player%} = "English":
					send "&c&oThis card's constellation is at max level." to player
				else:
					send "&c&oKonstelacja tej karty jest na maksymalnym poziomie." to player
	else if event-inventory = metadata tag "ConstellationAdd" of player:
		cancel event
		if index of clicked slot is between 0 and 26:
			if clicked slot is not air:
				set metadata tag "ConstellationAddConfirm" of player to chest inventory with 3 rows named "Constellation - Confirm"
				set slot 11 of metadata tag "ConstellationAddConfirm" of player to clicked slot
				set slot 15 of metadata tag "ConstellationAddConfirm" of player to lime glass pane named "&aConfirm?" with lore "&7Click here to confirm the process and%nl%&7advance the constellation level.%nl% %nl%&c&l(&4&l!&c&l) &c&nWarning!&c &oThis action will delete the card."
				open metadata tag "ConstellationAddConfirm" of player to player
	else if event-inventory = metadata tag "ConstellationAddConfirm" of player:
		cancel event
		if index of clicked slot is between 0 and 26:
			if clicked slot is not air:
				if clicked slot is lime glass pane named "&aConfirm?":
					loop lore of slot 11 of metadata tag "ConstellationAddConfirm" of player:
						if uncolored loop-value does not contain "(!)":
							add loop-value to {_loreItem::*}
					delete {_loreItem::%size of {_loreItem::*}%}
					set {_card} to slot 11 of metadata tag "ConstellationAddConfirm" of player with lore {_loreItem::*}
					loop all items in player's inventory:
						if loop-item = {_card}:
							set {_id} to uncolored line 19 of lore of loop-item
							replace all "Id: " in {_id} with ""
							set {_name} to uncolored name of loop-item
							unload yaml "plugins/Skript/playerdata/karty/%{_name}%/%{_id}%.yml"
							delete yaml "plugins/Skript/playerdata/karty/%{_name}%/%{_id}%.yml"
							remove loop-item from player
							teleport the player to player's location
							set {_idCard} to tag "Id" of nbt of player's tool
							load yaml "plugins/Skript/playerdata/karty/%uncolored name of player's tool%/%{_idCard}%.yml" as "karta-%player%"
							set {_constellation} to yaml value "Card.Constellation" in "karta-%player%"
							add 1 to {_constellation}
							set yaml value "Card.Constellation" in "karta-%player%" to {_constellation}
							save yaml "karta-%player%"
							unload yaml "karta-%player%"
							if {_constellation} = 1:
								set tag "Constellation;%uncolored name of slot 10 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							else if {_constellation} = 2:
								set tag "Constellation;%uncolored name of slot 11 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							else if {_constellation} = 3:
								set tag "Constellation;%uncolored name of slot 12 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							else if {_constellation} = 4:
								set tag "Constellation;%uncolored name of slot 13 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							else if {_constellation} = 5:
								set tag "Constellation;%uncolored name of slot 14 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							else if {_constellation} = 6:
								set tag "Constellation;%uncolored name of slot 15 of metadata tag "kartagui" of player%" of nbt of player's tool to true
							load yaml "plugins/Skript/playerdata/%uuid of player%.yml" as "playerDataConstellation-%player%"
							set {_constellations} to yaml value "Player.Total Constellations" in "playerDataConstellation-%player%"
							if {_constellations} is not set:
								set {_constellations} to 0
							add 1 to {_constellations}
							set yaml value "Player.Total Constellations" in "playerData-%player%" to {_constellations}
							save yaml "playerDataConstellation-%player%"
							unload yaml "playerDataConstellation-%player%"
							play sound "entity.player.levelup" with volume 3 and pitch 0.1 at player
							play sound "entity.firework_rocket.launch" with volume 3 and pitch 0.42 at player
							send "&b&l&l✨ &b&oSuccesfully advanced the constellation level of %name of player's tool%&b&o." to player
							loop 3 times:
								play sound "block.glass.break" with volume 3 and pitch 1.7+(loop-number*0.1) at player
							stop loop

