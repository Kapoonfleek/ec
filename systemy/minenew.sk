on rightclick:
	if clicked block is stone or deepslate:
		if player's tool is pickaxe:
			if "%region at player%" does not contain "spawn":
				if {umkilof::%player%} = "PomocnaDłoń":
					set {cdpdlon::%player%} to 10 seconds
					set {pdlon::cooldown::%player%} to difference between {pdlon::use::%player%} and now
					if {pdlon::cooldown::%player%} is less than {cdpdlon::%player%}:
						set {pdlontekst::%player%} to "%difference between {cdpdlon::%player%} and {pdlon::cooldown::%player%}%"
						replace all " seconds" and "seconds" in {pdlontekst::%player%} with " sekund"
						if {tekstcd::%player%} = true:
							if {language::%player%} = "English":
								send "&7You must wait &f%{pdlontekst::%player%}%&7to use this ability again." to player
							else:
								send "&7Musisz odczekać &f%{pdlontekst::%player%}%&7, aby użyć tej umiejętności." to player
						stop
					set {pdlon::use::%player%} to now
					break target block naturally
					set the cooldown of player's held item for player to 10 seconds
					minenew(player, "%clicked block%", clicked block)
					play sound "block.stone.break" with volume 3 and pitch 1 at clicked block
					if {gornictwo::poszumine::%player%} is true:
						chance of 1%:
							set {_f} to 1
							loop 3 times:
								play sound "block.note_block.xylophone" with volume 3 and pitch 1 to player
								add 0.3 to {_f}
								wait 0.3 seconds
							set {_czkamien} to a random integer between 1 and 11
							if {_czkamien} = 1:
								set event-block to red stained glass
							if {_czkamien} = 2:
								set event-block to lime stained glass
							if {_czkamien} = 3:
								set event-block to green stained glass
							if {_czkamien} = 4:
								set event-block to light blue stained glass
							if {_czkamien} = 5:
								set event-block to orange stained glass
							if {_czkamien} = 6:
								set event-block to yellow stained glass
							if {_czkamien} = 7:
								set event-block to magenta stained glass
							if {_czkamien} = 8:
								set event-block to purple stained glass
							if {_czkamien} = 9:
								set event-block to white stained glass
							if {_czkamien} = 10:
								set event-block to light gray stained glass
							if {_czkamien} = 11:
								set event-block to blue stained glass
							set {czystykamien::%event-block%} to true
							play sound "entity.player.levelup" with volume 3 and pitch 1 to player
							play sound "block.glass.break" with volume 3 and pitch 1.7 to player
				if {umkilof::%player%} = "Eksplozja":
					set {cdeksplok::%player%} to 2 minutes
					set {eksplok::cooldown::%player%} to difference between {eksplok::use::%player%} and now
					if {eksplok::cooldown::%player%} is less than {cdeksplok::%player%}:
						set {eksploktekst::%player%} to "%difference between {cdeksplok::%player%} and {eksplok::cooldown::%player%}%"
						replace all " seconds" and "seconds" in {eksploktekst::%player%} with " sekund"
						replace all " minutes" and "minutes" in {eksploktekst::%player%} with " minut"
						replace all " minute" and "minute" in {eksploktekst::%player%} with " minuta"
						if {tekstcd::%player%} = true:
							if {language::%player%} = "English":
								send "&7You must wait &f%{eksploktekst::%player%}%&7to use this ability again." to player
							else:
								send "&7Musisz odczekać &f%{eksploktekst::%player%}%&7, aby użyć tej umiejętności." to player
						stop
					set {eksplok::use::%player%} to now
					set the cooldown of player's held item for player to 2 minutes
					play sound "entity.generic.explode" with volume 3 and pitch 1 at clicked block
					loop all blocks in radius 3 around clicked block:
						if loop-block is stone:
							break loop-block naturally
							minenew(player, "%clicked block%", clicked block)
							play sound "block.stone.break" with volume 3 and pitch 0 at loop-block
							if {gornictwo::poszumine::%player%} is true:
								chance of 1%:
									set {_czkamien} to a random integer between 1 and 11
									if {_czkamien} = 1:
										set loop-block to red stained glass
									if {_czkamien} = 2:
										set loop-block to lime stained glass
									if {_czkamien} = 3:
										set loop-block to green stained glass
									if {_czkamien} = 4:
										set loop-block to light blue stained glass
									if {_czkamien} = 5:
										set loop-block to orange stained glass
									if {_czkamien} = 6:
										set loop-block to yellow stained glass
									if {_czkamien} = 7:
										set loop-block to magenta stained glass
									if {_czkamien} = 8:
										set loop-block to purple stained glass
									if {_czkamien} = 9:
										set loop-block to white stained glass
									if {_czkamien} = 10:
										set loop-block to light gray stained glass
									if {_czkamien} = 11:
										set loop-block to blue stained glass
									set {czystykamien::%loop-block%} to true
									play sound "entity.player.levelup" with volume 3 and pitch 1 to player
									play sound "block.glass.break" with volume 3 and pitch 1.7 to player
							wait 1 tick
			else:
				send "&8[&6Earthcraft&8] &7Musisz znajdować się poza spawnem, by móc użyć specjalnej umiejętności kilofa." to player
