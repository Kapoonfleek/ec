function ShadowyInsight(p: player):
    send title " " with subtitle "" to {_p} for 1 minute with fade in 0.25 second and fade out 0 second
    set metadata value "ShadowyInsight" of {_p} to true
    play sound "skill.therold.shadowy_insight_proc" with volume 2 and pitch 1 at {_p}
    clear cooldown "Mind Blast%{_p}%"
    clear cooldown "Shadow Word: Void%{_p}%"
    set {_time} to 60 seconds
    loop all items in the inventory of {_p}:
        if 1 of loop-item = light gray dye named "<##613e82>Mind Blast" with nbt from "{CustomModelData:3,MindBlast:1b}":
            reset item cooldown of loop-item for {_p}
        else if 1 of loop-item = light gray dye named "<##613e82>Shadow Word: Void" with nbt from "{CustomModelData:4,ShadowWordVoid:1b}":
            reset item cooldown of loop-item for {_p}
    while metadata value "ShadowyInsight" of {_p} = true:
        wait 1 second
        remove 1 second from {_time}
        stop loop if 0 seconds >= {_time}
    send title " " with subtitle " " to {_p} for 1 second with fade in 0.0 second and fade out 0.0 second

function isCastableTherold(p: player) :: boolean:
    if metadata value "ShadowyInsight" of {_p} = true:
        set {_castable} to true
    else:
        if tag "Talents;Row 1" of nbt of {karta::%{_p}%} = "Shadow Word: Void":
            if metadata value "ShadowWordVoidCharges" of {_p} > 0:
                set {_castable} to true
        else:
            if metadata value "MindBlastCharges" of {_p} > 0:
                set {_castable} to true
    if {_castable} is not set:
        set {_castable} to false
    return {_castable}

