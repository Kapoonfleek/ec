					if {_c} = 4:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■&8&l■■■■■■"
					if {_c} = 5:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■&8&l■■■■■"
					if {_c} = 6:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■■&8&l■■■■"
					if {_c} = 7:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■■■&8&l■■■"
					if {_c} = 8:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■■■■&8&l■■"
					if {_c} = 9:
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■■■■■&8&l■"
					if {_c} = 10:	
						set line 8 of {_p}'s fastboard to "    %{_kolorek}%&l■■■■■■■■■■"
					set line 9 of {_p}'s fastboard to ""
					set line 10 of {_p}'s fastboard to "  &f&l«-───▬ %{_kolorek}%&lEC &f&l▬───-»"
			else:
				islandExpeditionFastboard({_p})
