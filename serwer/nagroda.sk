					set {_karta} to black stained glass pane named "<##555555>Umbreon" with lore "&7Night Huntress" and {_statsEN} and "&7Element: <##333333>Dark" and "&7Source: <##555555>Starter" with nbt compound from "{""minecraft:custom_model_data"":{floats:[1.0f]}}"
					set {_x} to "glow ink sac" parsed as item
