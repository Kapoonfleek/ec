command /partci:
	trigger:
		set {_x} to block at location(-1234.5, 80.5, -1839.5, world "world")
		set {_y} to 0
		if chunk at {_x} is loaded:
			loop 1000 times:
				loop 120 times:
					set {_v} to vector 1, 0, 1
					rotate {_v} around y-axis by {_y}
					add 4 to {_y}
					draw 1 of dust using dustOption(red, 1) at {_x} ~ {_v}
					wait 1 tick

command /uuid [<text>]:
	trigger:
		if length of arg-1 >= 16:
			set {_x} to arg-1 parsed as player
			if {_x} is set:
				send "&fPlayer: %{_x}%" to player
			else:
				send "&fNo player with the UUID %arg-1% found." to player
		else:
			set {_p} to arg-1 parsed as player
			set {_x} to uuid of {_p}
			if {_x} is set:
				send "&fUUID of %{_p}%: %{_x}%"
			else:
				send "&fThe player does not exist in the server's database." to player
