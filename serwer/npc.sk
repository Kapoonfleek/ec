on inventory click:
	if player's current inventory = metadata tag "isabela" of player:
		cancel event
		set {_id} to line 18 of lore of {karta::%player%}
		set {_list::*} to 19, 20, 21, 22, 23, 24, 25, 29, 30, 31, 32 and 33
		if {_list::*} contains clicked raw slot:
			if {dostepnestaty::%{_id}%} > 0:
				play sound "entity.player.levelup" with volume 5 and pitch 0.22 at player
				if clicked raw slot = 19:
					remove 1 from {dostepnestaty::%{_id}%}
					add 1 to {zdrowie::%{_id}%}
					add 1 to {zdrowie::poziom::%{_id}%}
