	add 0.5 to {exp::%player%}
	add 0.5 to {expkopanie::%player%}
	UpdateScoreboard(player)
on mine of granite:
	add 0.5 to {exp::%player%}
	add 0.5 to {expkopanie::%player%}
	UpdateScoreboard(player)
on mine of tuff:
	add 0.5 to {exp::%player%}
	add 0.5 to {expkopanie::%player%}
	UpdateScoreboard(player)
on mine of quartz ore:
	add 1.5 to {exp::%player%}
	add 1.5 to {expkopanie::%player%}
	add 1 to {kwarc::%player%}
	UpdateScoreboard(player)
on player pickup xp:
	if {orbbudget::%player%} > 0:
		cancel event
		loop all experience orbs in radius 2 around the player:
			kill loop-value
			stop loop
		remove 1 from {orbbudget::%player%}
		add 1 to {orbcount::%player%}
		play sound "entity.experience_orb.pickup" with volume 2 and pitch 2 to player
	else:
		if {orbbudget::%player%} is set:
			cancel event
			loop all experience orbs in radius 2 around the player:
				kill loop-value
				stop loop
on xp orb merge:
	cancel event
on sign change:
	line 1 is "[Elevator]":
		line 2 is "Lift Up":
			set line 1 to "&1[Elevator]"
			set line 2 to "Lift Up"
	line 1 is "[Elevator]":
		line 2 is "Lift Down":
			set line 1 to "&1[Elevator]"
			set line 2 to "Lift Down"
on rightclick on sign:
	line 1 is "&1[Elevator]":
		line 2 is "Lift Up":
			execute player command "/ascend"
		line 2 is "Lift Down":
			execute player command "/descend"
on leaves decay:
	cancel event
on skript load:
	execute console command "/whitelist on"
command /levelreset:
	trigger:
		send "&8[&aEarthcraft&8] &aZresetowano poziom&7!"
		clear {poziom::%player%}
		clear {exp::%player%}