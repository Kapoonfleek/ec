function VoidEruptionAbilityCast(p: player):
    if {kartaname::%{_p}%} = "Brother Therold":
        if CanUseAbility({_p}) != false:
            if metadata value "casting" of {_p} is not set:
                if tag "Talents;Row 5" of nbt of {karta::%{_p}%} = "Legacy of the Void":
                    set {_requiredInsanity} to 60
                else:
                    set {_requiredInsanity} to 90
                if {insanity::%{_p}%} >= {_requiredInsanity}:
                    set {_target} to Target({_p}, 100, false)
                    if {_target} is set:
                        if cooldown "Shadow Word: Pain%{_p}%" is finished:
                            if cooldown "Shadow Word: Death%{_p}%" is finished:
                                if cooldown "Global Cooldown Interrupted Cast%{_p}%" is finished:
                                    set {_id} to line 19 of lore of {karta::%{_p}%}
                                    set {_pospiech} to 1 + (({haste::%{_p}%}/4))/100
                                    set {_baseCasttime} to 2
                                    if tag "Talents;Row 5" of nbt of {karta::%{_p}%} = "Legacy of the Void":
                                        set {_baseCasttime} to {_baseCasttime}*0.6 
                                    set {_casttime} to "%round(({_baseCasttime}/{_pospiech}), 1)% seconds" parsed as timespan
                                    if metadata value "cast" of {_p} is not set:
                                        set metadata value "casttimemax" of {_p} to {_casttime}
                                        set metadata value "lastspell" of {_p} to {_p}'s tool's name
                                        play sound "skill.therold.void_eruption_precast" with volume 3 and pitch 1 at {_p}
                                        set metadata value "casting" of {_p} to true
                                        set metadata value "cast" of {_p} to {_casttime}
                                        while metadata value "casting" of {_p} = true:
                                            if metadata value "channeling" of {_p} is set:
                                                clear metadata value "channeling" of {_p}
                                                stop sound "skill.therold.mindflay_channel_whisper" for all players in radius 2 around {_p}
                                                stop sound "skill.therold.mindflay_channel" for all players in radius 2 around {_p}
                                            set {_voidformStacks} to metadata value "Voidform Stacks" of {_p}
                                            set {_insanityDrain} to 6 + 0.68*{_voidformStacks}
                                            set {_insanityInfo} to "Insanity: %round({insanity::%{_p}%}, 0)%"
                                            if metadata value "Voidform" of {_p} is set:
                                                set {_insanityInfo} to "%{_insanityInfo}%   %round({_insanityDrain}, 1)%p/s    %{_voidformStacks}%"
                                            if tag "Talents;Row 3" of nbt of {karta::%{_p}%} = "Lingering Insanity":
                                                if {lingeringInsanityHaste::%{_p}%} is set:
                                                    set {_insanityInfo} to "%{_insanityInfo}% |  %{lingeringInsanityHaste::%{_p}%}/4%%% "
                                            if metadata value "cast" of {_p} is not set:
                                                clear metadata value "casting" of {_p}
                                                stop
                                            if {_casttime} > 0 seconds:
                                                set metadata value "casttimeleft" of {_p} to {_casttime}
                                                set {_cst} to "%{_casttime}%"
                                                replace all "seconds" and "second" in {_cst} with "sekund"
                                                if {language::%{_p}%} = "English":
                                                    send action bar "Casting... %{_cst}% | %{_insanityInfo}%" to {_p}
                                                else:
                                                    replace all "seconds" and "second" in {_cst} with "sekund"
                                                    send action bar "Rzucanie Zaklęcia... %{_cst}% | %{_insanityInfo}%" to {_p}
                                                wait 0.1 seconds
                                                remove 0.1 seconds from {_casttime}
                                            else:
                                                clear metadata value "casting" of {_p}
                                                if {language::%{_p}%} = "English":
                                                    send action bar "" to {_p}
                                                else:
                                                    send action bar "" to {_p}
                                    else:
                                        stop
                                    clear metadata value "casting" of {_p}
                                    clear metadata value "cast" of {_p}
                                    clear metadata value "casttime" of {_p}
                                    clear metadata value "casttimemax" of {_p}
                                    stop sound "skill.therold.void_eruption_precast" for all players in radius 2 around {_p}
                                    if {_casttime} > 0 seconds:
                                        if {language::%{_p}%} = "English":
                                            send action bar "&cInterrupted" to {_p}
                                        else:
                                            send action bar "&cPrzerwano" to {_p}
                                        stop
                                    set metadata value "Voidform" of {_p} to true
                                    AbilityUse({_p}, "Void Eruption")
                                    play sound "skill.therold.void_eruption_cast" with volume 3 and pitch 1 at {_p}
                                    set {_crit} to 0 + ({criticalhit::%{_p}%}/10)
                                    loop all items in the inventory of {_p}:
                                        if loop-item is purple dye named "<##613e82>Void Eruption" with nbt from "{CustomModelData:9,VoidEruption:1b}":
                                            set {_cdVoidbolt} to "%4.5/{_pospiech}% seconds" parsed as timespan
                                            set {_dmgVoidbolt} to (3 + (({damage::%{_p}%}/15) + ({versatility::%{_p}%}/45)))
                                            set loop-item to black dye named "<##613e82>Void Bolt" with lore "&8Ultimate Ability%nl%&fInstant cast%nl%&7Cooldown: &f%{_cdVoidbolt}%%nl%&7Use: &fRMB%nl%<##613e82>Sends a bolt of pure void energy at the%nl%<##613e82>enemy, causing &f%{_dmgVoidbolt}% <##431d53>Ghost <##613e82>damage and%nl%<##613e82>extending the duration of <##8165a6>Shadow Word: Pain%nl%<##613e82>and <##8165a6>Vampiric Touch <##613e82>to their original duration.%nl%<##613e82>Requires <##8165a6>Voidform<##613e82>.%nl% %nl%&fGenerates 16 Insanity." with nbt from "{CustomModelData:3,VoidBolt:1b}"
                                    set {_lore} to join lore of {_p}'s tool by nl
                                    apply levitation 0.5 without particles to {_p} for 1 day
                                    loop all entities in radius 8 around {_target}:
                                        if loop-entity is a living entity:
                                            if loop-entity is alive:
                                                if loop-entity is not a player:
                                                    if tag "custom;ECMob" of nbt of loop-entity is set:
                                                        if CheckIfDotted({_p}, loop-entity) is true:
                                                            VoidEruptionVisual({_p}, loop-entity)
                                                            set {_damage} to (4 + (({damage::%{_p}%}/9) + ({versatility::%{_p}%}/27) + {primarystat::%{_p}%}/4.5)) * (1.096 + (({mastery::%{_p}%}/6.25)/100))
                                                            set loop-entity's last damage cause to sweep attack
                                                            set metadata value "LastDamageCauseEN" of loop-entity to "<##462566>Void Eruption"
                                                            set metadata value "LastDamageCause" of loop-entity to "<##462566>Erupcja Pustki"
                                                            set metadata value "LastDamageCauseLore" of loop-entity to {_lore}
                                                            set metadata value "LastDamageCauseLoreEN" of loop-entity to {_lore}
                                                            set metadata value "Element" of {_p} to "Ghost"
                                                            chance of {_crit}%:
                                                                set {_damage} to {_damage}*2
                                                            make {_p} damage loop-entity by {_damage}*2
                                    while metadata value "Voidform" of {_p} is true:
                                        if {insanity::%{_p}%} > 0:
                                            add 1 to {_voidformStacks}
                                            set metadata value "Voidform Stacks" of {_p} to {_voidformStacks}
                                            set {_insanityDrain} to 6 + 0.68*{_voidformStacks}
                                            remove {_insanityDrain} from {insanity::%{_p}%}             
                                            draw 20 dust using dustOption(purple, 1) at location above {_p} with offset vector(0.3, 0.6, 0.3) with extra 0.03
                                            set {insanity::%{_p}%} to 0 if {insanity::%{_p}%} < 0
                                            add 4 to {hasteVoidform::%{_p}%}
                                        Mania({_p})
                                        ExitVoidform({_p}) if {insanity::%{_p}%} = 0
                                        stop if {insanity::%{_p}%} = 0
                                        set {_insanityInfo} to "Insanity: %round({insanity::%{_p}%}, 0)%"
                                        set {_insanityInfo} to "%{_insanityInfo}%   %round({_insanityDrain}, 1)%p/s    %{_voidformStacks}%"
                                        if tag "Talents;Row 3" of nbt of {karta::%{_p}%} = "Lingering Insanity":
                                            if {lingeringInsanityHaste::%{_p}%} is set:
                                                set {_insanityInfo} to "%{_insanityInfo}% |  %{lingeringInsanityHaste::%{_p}%}/4%%% "
                                        send action bar {_insanityInfo} to {_p}
                                        wait 1 second
                            else: 
                                if {tekstcd::%{_p}%} = true:
                                    set {_cdd} to cooldown "Shadow Word: Death%{_p}%"
                                    if {language::%{_p}%} = "English":
                                        send "<##462566>You must wait &f%{_cdd}%<##462566> to use this ability." to {_p}
                                    else:
                                        send "<##462566>Musisz odczekać &f%{_cdd}%<##462566>, aby użyć tej umiejętności." to {_p}
                                stop
                        else: 
                            if {tekstcd::%{_p}%} = true:
                                set {_cdd} to cooldown "Shadow Word: Pain%{_p}%"
                                if {language::%{_p}%} = "English":
                                    send "<##462566>You must wait &f%{_cdd}%<##462566> to use this ability." to {_p}
                                else:
                                    send "<##462566>Musisz odczekać &f%{_cdd}%<##462566>, aby użyć tej umiejętności." to {_p}
                            stop
    else:
        if {language::%{_p}%} = "English":
            send "&8[&6Earthcraft&8] &6You don't have the required card selected to use this ability." to {_p}
        else:
            send "&8[&6Earthcraft&8] &6Nie masz wybranej wymaganej karty, by móc użyć tej umiejętności." to {_p}
        play sound "block.glass.break" with volume 3 and pitch 0 to {_p}

function VoidBoltAbilityCast(p: player):
    if {kartaname::%{_p}%} = "Brother Therold":
        if CanUseAbility({_p}) != false:
            set {_target} to Target({_p}, 100, false)
            if {_target} is set:
                if cooldown "Void Bolt%{_p}%" is finished:
                    if cooldown "Shadow Word: Pain%{_p}%" is finished:
                        if cooldown "Shadow Word: Death%{_p}%" is finished:
                            if cooldown "Global Cooldown Interrupted Cast%{_p}%" is finished:
                                if metadata value "Voidform" of {_p} is set:
                                    set {_id} to line 19 of lore of {karta::%{_p}%}
                                    set {_pospiech} to 1 + (({haste::%{_p}%}/4) + ({hasteVoidform::%{_p}%}/4))/100
                                    set {_cd} to "%4.5/{_pospiech}% seconds" parsed as timespan
                                    set the cooldown of {_p}'s held item for {_p} to {_cd}
                                    create cooldown "Void Bolt%{_p}%" for {_cd}
                                    AbilityUse({_p}, "Void Bolt")
                                    if metadata value "channeling" of {_p} is set:
                                        clear metadata value "channeling" of {_p}
                                        StopCastSoundTherold({_p})
                                    if metadata value "casting" of {_p} is set:
                                        clear metadata value "cast" of {_p}
                                        StopCastSoundTherold({_p})
                                        if {language::%{_p}%} = "English":
                                            send action bar "&cInterrupted" to {_p}
                                        else:
                                            send action bar "&cPrzerwano" to {_p}
                                    set metadata value "lastspell" of {_p} to {_p}'s tool's name
                                    set {_randomLocation} to a random number between -0.40 and 0.40
                                    set {_l} to location 0.3 blocks below and {_randomLocation} left of {_p}'s head
                                    set {_vec} to vector between {_p} and {_target}'s head
                                    play sound "skill.therold.void_bolt_cast" with volume 3 and pitch 1 at {_p}
                                    play sound "skill.therold.shadowcast" with volume 3 and pitch 1 at {_p}
                                    loop round(distance between {_l} and {_target}) / 0.5 times:
                                        set {_vec} to vector between {_l} and {_target}'s head
                                        set vector length of {_vec} to loop-value * 0.5
                                        draw 1 of dust using dustOption(rgb(65, 46, 115), 1) at {_l} ~ {_vec} with force
                                    play sound "skill.therold.void_bolt_impact" with volume 3 and pitch 1 at {_p}
                                    GrantInsanity({_p}, 16)
                                    UpdateInsanity({_p})
                                    set {_crit} to 0 + ({criticalhit::%{_p}%}/10)
                                    set {_damage} to (4.5 + (({damage::%{_p}%}/14.5) + ({versatility::%{_p}%}/43.5) + {primarystat::%{_p}%}/7.25)) * (1.096 + (({mastery::%{_p}%}/6.25)/100))
                                    if metadata value "Voidform" of {_p} is set:
                                        set {_damage} to {_damage}*1.2
                                    set {_target}'s last damage cause to sweep attack
                                    set {_lore} to "&8Ultimate Ability%nl%&fInstant cast%nl%&7Cooldown: &f%{_cd}%%nl%&7Use: &fRMB%nl%<##613e82>Sends a bolt of pure void energy at the%nl%<##613e82>enemy, causing &f%{_damage}% <##431d53>Ghost <##613e82>damage and%nl%<##613e82>extending the duration of <##8165a6>Shadow Word: Pain%nl%<##613e82>and <##8165a6>Vampiric Touch <##613e82>to their original duration.%nl%<##613e82>Requires <##8165a6>Voidform<##613e82>.%nl% %nl%&fGenerates 16 Insanity."
                                    set metadata value "LastDamageCauseEN" of {_target} to "<##462566>Void Bolt"
                                    set metadata value "LastDamageCause" of {_target} to "<##462566>Bełt Pustki"
                                    set metadata value "LastDamageCauseLore" of {_target} to {_lore}
                                    set metadata value "LastDamageCauseLoreEN" of {_target} to {_lore}
                                    set metadata value "Element" of {_p} to "Ghost"
                                    set lore of {_p}'s tool to {_lore}
                                    chance of {_crit}%:
                                        set {_damage} to {_damage}*2
                                    make {_p} damage {_target} by {_damage}*2
                                    loop all entities in radius 4 around {_target}:
                                        if metadata value "Shadow Word: Pain-%{_p}%" of loop-entity is set:
                                            ShadowWordPainRefresh({_p}, loop-entity)
                                        if metadata value "Vampiric Touch-%{_p}%" of loop-entity is set:
                                            VampiricTouchRefresh({_p}, loop-entity)
                                else:
                                    if {language::%{_p}%} = "English":
                                        send "<##462566>You must be in Voidform to use this ability." to {_p}
                                    else:
                                        send "<##462566>Musisz być w Formie Pustki, by użyć tej umiejętności." to {_p}
                                    stop
                        else: 
                            if {tekstcd::%{_p}%} = true:
                                set {_cdd} to cooldown "Shadow Word: Death%{_p}%"
                                if {language::%{_p}%} = "English":
                                    send "<##462566>You must wait &f%{_cdd}%<##462566> to use this ability." to {_p}
                                else:
                                    send "<##462566>Musisz odczekać &f%{_cdd}%<##462566>, aby użyć tej umiejętności." to {_p}
                            stop
                    else: 
                        if {tekstcd::%{_p}%} = true:
                            set {_cdd} to cooldown "Shadow Word: Pain%{_p}%"
                            if {language::%{_p}%} = "English":
                                send "<##462566>You must wait &f%{_cdd}%<##462566> to use this ability." to {_p}
                            else:
                                send "<##462566>Musisz odczekać &f%{_cdd}%<##462566>, aby użyć tej umiejętności." to {_p}
                        stop
                else: 
                    if {tekstcd::%{_p}%} = true:
                        set {_cdd} to cooldown "Void Bolt%{_p}%"
                        if {language::%{_p}%} = "English":
                            send "<##462566>You must wait &f%{_cdd}%<##462566> to use this ability." to {_p}
                        else:
                            send "<##462566>Musisz odczekać &f%{_cdd}%<##462566>, aby użyć tej umiejętności." to {_p}
                    stop
    else:
        if {language::%{_p}%} = "English":
            send "&8[&6Earthcraft&8] &6You don't have the required card selected to use this ability." to {_p}
        else:
            send "&8[&6Earthcraft&8] &6Nie masz wybranej wymaganej karty, by móc użyć tej umiejętności." to {_p}
        play sound "block.glass.break" with volume 3 and pitch 0 to {_p}

function MindBlastAbilityCast(p: player):
    if {kartaname::%{_p}%} = "Brother Therold":
        if tag "Talents;Row 1" of nbt of {karta::%{_p}%} != "Shadow Word: Void":
            if CanUseAbility({_p}) != false:
                if metadata value "casting" of {_p} is not set:
                    set {_target} to Target({_p}, 100, false)
                    if {_target} is set:
                        if isCastableTherold({_p}) = true:
                            if cooldown "Shadow Word: Pain%{_p}%" is finished:
                                if cooldown "Shadow Word: Death%{_p}%" is finished:
                                    if cooldown "Global Cooldown Interrupted Cast%{_p}%" is finished:
                                        set {_id} to line 19 of lore of {karta::%{_p}%}
                                        set {_pospiech} to 1 + (({haste::%{_p}%}/4) + ({hasteVoidform::%{_p}%}/4))/100
                                        set {_casttime} to "%round((1.5/{_pospiech}), 1)% seconds" parsed as timespan
                                        set {_casttimeText} to {_casttime}
                                        set {_cd} to "%9/{_pospiech}% seconds" parsed as timespan
                                        if metadata value "ShadowyInsight" of {_p} != true:
                                            if metadata value "cast" of {_p} is not set:
                                                set metadata value "casttimemax" of {_p} to {_casttime}
                                                play sound "skill.therold.mindblast_precast" with volume 3 and pitch 1 at {_p}
                                                set metadata value "casting" of {_p} to true
                                                if metadata value "channeling" of {_p} is set:
                                                    clear metadata value "channeling" of {_p}
                                                    StopCastSoundTherold({_p})
                                                set metadata value "lastspell" of {_p} to {_p}'s tool's name
                                                set metadata value "cast" of {_p} to {_casttime}
                                                while metadata value "casting" of {_p} = true:
                                                    set {_voidformStacks} to metadata value "Voidform Stacks" of {_p}
                                                    set {_insanityDrain} to 6 + 0.68*{_voidformStacks}
                                                    set {_insanityInfo} to "Insanity: %round({insanity::%{_p}%}, 0)%"
                                                    if metadata value "Voidform" of {_p} is set:
                                                        set {_insanityInfo} to "%{_insanityInfo}%   %round({_insanityDrain}, 1)%p/s    %{_voidformStacks}%"
                                                    if tag "Talents;Row 3" of nbt of {karta::%{_p}%} = "Lingering Insanity":
                                                        if {lingeringInsanityHaste::%{_p}%} is set:
                                                            set {_insanityInfo} to "%{_insanityInfo}% |  %{lingeringInsanityHaste::%{_p}%}/4%%% "
                                                    if metadata value "cast" of {_p} is not set:
                                                        clear metadata value "casting" of {_p}
                                                        stop
                                                    if {_casttime} > 0 seconds:
                                                        set metadata value "casttimeleft" of {_p} to {_casttime}
                                                        set {_cst} to "%{_casttime}%"
                                                        replace all "seconds" and "second" in {_cst} with "sekund"
                                                        if {language::%{_p}%} = "English":
                                                            send action bar "Casting... %{_cst}% | %{_insanityInfo}%" to {_p}
                                                        else:
                                                            replace all "seconds" and "second" in {_cst} with "sekund"
                                                            send action bar "Rzucanie Zaklęcia... %{_cst}% | %{_insanityInfo}%" to {_p}
                                                        wait 0.1 seconds
                                                        remove 0.1 seconds from {_casttime}
                                                    else:
                                                        clear metadata value "casting" of {_p}
                                                        if {language::%{_p}%} = "English":
                                                            send action bar "" to {_p}
                                                        else:
                                                            send action bar "" to {_p}
                                            else:
                                                stop
                                            clear metadata value "casting" of {_p}
                                            clear metadata value "cast" of {_p}
                                            clear metadata value "casttimeleft" of {_p}
                                            clear metadata value "casttimemax" of {_p}
                                            stop sound "skill.therold.mindblast_precast" for all players in radius 2 around {_p}
                                            if {_casttime} > 0 seconds:
                                                if {language::%{_p}%} = "English":
                                                    send action bar "&cInterrupted" to {_p}
                                                else:
                                                    send action bar "&cPrzerwano" to {_p}
                                                stop
                                            set {_charges} to metadata value "MindBlastCharges" of {_p}
                                            remove 1 from {_charges}
                                            set metadata value "MindBlastCharges" of {_p} to {_charges}
                                            if tag "Constellation;Thought Harvester" of nbt of {karta::%{_p}%} is not set:
                                                create cooldown "Mind Blast%{_p}%" for {_cd}
                                            MindBlastRecharge({_p}, {_cd})
                                        else:
                                            clear metadata value "ShadowyInsight" of {_p}
                                            set {_charges} to metadata value "MindBlastCharges" of {_p}
                                            send title " " with subtitle " " to {_p} for 1 second with fade in 0.0 second and fade out 0.0 second
                                        AbilityUse({_p}, "Mind Blast")
                                        set {_insanity} to 12
                                        if tag "Talents;Row 1" of nbt of {karta::%{_p}%} = "Fortress of the Mind":
                                            set {_insanity} to {_insanity}*1.2
                                        GrantInsanity({_p}, {_insanity})
                                        UpdateInsanity({_p})
                                        play sound "skill.therold.mindblast_impact" with volume 3 and pitch 1 at {_p}
                                        play sound "skill.therold.shadowcast" with volume 3 and pitch 1 at {_p}
                                        draw 20 dust using dustOption(rgb(95, 72, 156), 1) at {_target}'s head with offset vector(0.4, 0.5, 0.4) with extra 0.03
                                        draw 20 dust using dustOption(rgb(41, 22, 92), 1) at {_target}'s head with offset vector(0.35, 0.45, 0.35) with extra 0.03
                                        if {_target} is alive:
                                            set {_crit} to 0 + ({criticalhit::%{_p}%}/10)
                                            set {_damage} to (4.5 + (({damage::%{_p}%}/24) + ({versatility::%{_p}%}/72) + {primarystat::%{_p}%}/12)) * (1.096 + (({mastery::%{_p}%}/6.25)/100))
                                            if metadata value "Voidform" of {_p} is set:
                                                set {_damage} to {_damage}*1.2
                                            if tag "Talents;Row 1" of nbt of {karta::%{_p}%} = "Fortress of the Mind":
                                                set {_damage} to {_damage}*1.1
                                            if {_charges} = 1:
                                                set {_chargesLore} to "%{_charges}% Charge"
                                            else:
                                                set {_chargesLore} to "%{_charges}% Charges"
                                            if tag "Constellation;Thought Harvester" of nbt of {karta::%{_p}%} is set:
                                                set {_lore} to "&8Ability%nl%&f%{_chargesLore}%%nl%&f%{_casttimeText}% cast%nl%&7Recharge time: &f%{_cd}%%nl%&7Use: &fRMB%nl%<##613e82>Blasts the target's mind for &f%{_damage}%%nl%<##431d53>Ghost <##613e82>damage.%nl% %nl%&fGenerates 12 Insanity."
                                            else:
                                                set {_lore} to "&8Ability%nl%&f%{_casttimeText}% cast%nl%&7Cooldown: &f%{_cd}%%nl%&7Use: &fRMB%nl%<##613e82>Blasts the target's mind for &f%{_damage}%%nl%<##431d53>Ghost <##613e82>damage.%nl% %nl%&fGenerates 12 Insanity."
                                            loop all items in the inventory of {_p}:
                                                if loop-item = light gray dye named "<##613e82>Mind Blast" with nbt from "{CustomModelData:3,MindBlast:1b}":
                                                    set lore of loop-item to {_lore}
                                                    if {_charges} > 0:
                                                        set item amount of loop-item to {_charges}
                                                    else:
                                                        set the cooldown of loop-item for {_p} to {_cd}
                                                    stop loop
